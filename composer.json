{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb": "^1.6",
        "doctrine/mongodb-odm": "^1.2",
        "doctrine/mongodb-odm-bundle": "^3.4",
        "easycorp/easy-log-handler": "^1.0.3",
        "friendsofsymfony/rest-bundle": "^2.3",
        "knplabs/knp-paginator-bundle": "^2.7",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^5.1.3",
        "symfony/console": "^4.0.2",
        "symfony/debug-bundle": "^4.0.2",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0.2",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/phpunit-bridge": "^4.0.2",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-guard": "^4.0",
        "symfony/serializer": "^4.0.2",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.0.2",
        "symfony/validator": "^4.0.2",
        "symfony/var-dumper": "^4.0.2",
        "symfony/web-server-bundle": "^4.0.2",
        "symfony/yaml": "^4.0.2",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0RMFC1RJ0MMBW38T07NQA1P",
            "allow-contrib": false
        }
    }
}
